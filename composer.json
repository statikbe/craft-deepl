{
    "name": "statikbe/craft-deepl", 
    "description": "DeepL integration for Craft CMS",
    "type": "craft-plugin",
    "version": "1.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "translate"
    ],
    "support": {
        "docs": "https://github.com/statikbe/craft-deepl/blob/main/README.md",
        "issues": "https://github.com/statikbe/craft-deepl/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Statik",
            "homepage": "https://www.statik.be"
        },
        {
            "name": "Jan Henckens",
            "email": "jan@statik.be"
        }
    ],
    "require": {
        "craftcms/cms": "^4.0.0",
        "deeplcom/deepl-php": "^v0.4.0"
    },
    "autoload": {
        "psr-4": {
          "statikbe\\deepl\\": "src/"
        }
    },
    "extra": {
        "name": "DeepL",
        "handle": "deepl",
        "schemaVersion": "1.0.0",
        "hasSettings": true,
        "developer": "Statik",
        "class": "statikbe\\deepl\\Deepl"
    }
}
